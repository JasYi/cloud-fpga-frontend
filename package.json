    "electron": "^28.2.3",
    "fs": "^0.0.1-security",
    "plotly": "^1.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react-scripts": "^5.0.1",
    "require-polyfills": "^1.0.1",
    "vcd-parser": "^1.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
